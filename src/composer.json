{
  "require": {
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.3",
    "rych/random": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
